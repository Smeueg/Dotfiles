{
    "diagnostics": {
        "enable": true,
        "globals": [
            "awesome",
            "button",
            "dbus",
            "client",
						"tag",
            "screen",
            "mouse",
            "root",
        ],
    },
    "runtime": {
        "version": "LuaJIT",
				"path": [
            "/usr/share/awesome/lib/?.lua",
            "/usr/share/awesome/lib/?/init.lua",
        ]
    },
    "workspace": {
        "library": [
            "runtime/lua",
            "/usr/share/awesome/lib",
        ],
        "checkThirdParty": false,
        "maxPreload": 2000,
        "preloadFileSize": 1000
    },
    "telemetry.enable": false
}
